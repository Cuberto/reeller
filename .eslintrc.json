{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": ["eslint:recommended"],
    "parserOptions": {
        "ecmaVersion": 2022,
        "sourceType": "module"
    },
    "ignorePatterns": ["node_modules", "dist/", "src/scss/", "tmp"],
    "rules": {
        "require-jsdoc": "off",
        "indent": ["error", 4],
        "no-prototype-builtins": "off",
        "max-len": [
            "error",
            {
                "code": 120
            }
        ]
    }
}
